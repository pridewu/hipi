<form id="syslog_form" method="post" style="padding:10px">
	<table class="editTable">
		<tr><th>ID</th><td>{$log['id']}</td><th>管理员</th><td>{$log['userName']}</td></tr>
		<tr><th>页面</th><td>{$log['controller']}</td><th>操作</th><td>{$log['action']}</td></tr>
		<tr><th>时间</th><td colspan="3">{$log['addTime']}</td></tr>
		<tr><th>内容</th><td colspan="3">{$log['data']}</td></tr>
	</table>	
</form>